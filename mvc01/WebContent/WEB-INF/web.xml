<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>mvc01</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>ex03</servlet-name>
  	<servlet-class>com.hb.day01.Ex03</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ex05</servlet-name>
  	<servlet-class>com.hb.day01.Ex05</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>list</servlet-name>
  	<servlet-class>com.hb.day01.ListJson</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ex03</servlet-name>
  	<url-pattern>/ex03</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ex05</servlet-name>
  	<url-pattern>/ex05.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>list</servlet-name>
  	<url-pattern>/list.json</url-pattern>
  </servlet-mapping>
</web-app>






